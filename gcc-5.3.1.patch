+
+    write (buffer, "(2a3)") array_fijo
+
+!    WRITE(*,*) ' Escriba un numero para el largo de cada linea'
+!    READ(*,*) largo
+    largo = LEN (array_fijo)
+
+!    WRITE(*,*) ' Escriba la cantidad de lineas'
+!    READ(*,*) cant_lineas
